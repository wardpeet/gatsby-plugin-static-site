{
  "name": "@wardpeet/gatsby-plugin-static-site",
  "version": "0.1.1",
  "description": "A plugin that disables client side routing for gatsby",
  "main": "index.js",
  "author": "Ward Peeters <ward@coding-tech.com>",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "static-site"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@reach/router": "^1.2.1",
    "gatsby": "^2.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cpy": "^7.3.0",
    "cross-env": "^5.1.4",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "globby": "^10.0.1",
    "make-dir": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "lint": "npm-run-all --parallel lint:*",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:prettier": "npm run prettier -- --check",
    "format": "npm-run-all --parallel format:*",
    "format:code": "npm run lint:code -- --fix",
    "format:prettier": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.{md,css,yml,json}\""
  }
}