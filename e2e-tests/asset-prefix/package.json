{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "cypress": "^6.0.0",
    "express": "^4.17.1",
    "gatsby": "^2.19.49",
    "gatsby-image": "^2.2.45",
    "gatsby-plugin-manifest": "^2.2.48",
    "gatsby-plugin-offline": "^3.0.42",
    "gatsby-plugin-react-helmet": "^3.1.24",
    "gatsby-plugin-sharp": "^2.4.13",
    "gatsby-source-filesystem": "^2.1.57",
    "gatsby-transformer-sharp": "^2.3.19",
    "polka": "^0.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "gatsby-cypress": "0.11.0",
    "start-server-and-test": "1.12.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "node ../../scripts/copy-plugin.js",
    "build": "cross-env CYPRESS_SUPPORT=y gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "node server.js",
    "test:cypress": "cypress run",
    "test": "start-server-and-test serve http://localhost:3000 test:cypress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
