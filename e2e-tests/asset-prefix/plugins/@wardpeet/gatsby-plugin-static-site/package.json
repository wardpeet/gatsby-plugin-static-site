{
  "name": "@wardpeet/gatsby-plugin-static-site",
  "version": "0.1.1",
  "description": "A plugin that disables client side routing for gatsby",
  "main": "index.js",
  "author": "Ward Peeters <ward@coding-tech.com>",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "static-site"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@reach/router": "^1.2.1",
    "gatsby": "^2.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cpy": "^7.3.0",
    "cross-env": "^5.1.4",
    "globby": "^10.0.1",
    "make-dir": "^3.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
